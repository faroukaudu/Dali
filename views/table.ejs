  	<title>Table 05</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<link href='https://fonts.googleapis.com/css?family=Roboto:400,100,300,700' rel='stylesheet' type='text/css'>

	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
	
	<link rel="stylesheet" href="assets-table/css/style.css">

	</head>
	<body>
		<div class="ugf-nav">
			<div class="container">
			  <div class="row">
				<div class="col-lg-8 offset-lg-2">
				  <div class="navigation">
					<div class="logo">
					  <a href="../index.html"><img src="assets-form/images/logo-dark.png" class="img-fluid" alt=""><span>KYC</span></a>
					</div>
					<div class="nav-btns">
					  <a href="../index.html" class="back"><span class="text">Back to Main</span> Demo</a>
					  <a href="https://themeforest.net/item/anfra-questionnaire-form-wizard/29917051" class="get">Create a New Campaign</a>
					</div>
				  </div>
				</div>
			  </div>
			</div>
		  </div>
	<section class="ftco-section">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-md-6 text-center mb-5">
					<h2 class="heading-section">Table #05</h2>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12">
					<div class="table-wrap">
						<table class="table table-responsive-xl">
						  <thead>
						    <tr>
						    	<!-- <th>s/n</th> -->
						    	<th>Ads Campaign</th>
						      <th>State</th>
							  <th>Site</th>
							  <th>Structure</th>
							  <th>“₦” Unit/M</th>
							  <th>End Date</th>
						      <th>Status / Action</th>
						      
							
						    </tr>
						  </thead>
						  <tbody>
						
						    
							<% items.forEach(function(item){ %>
								<tr class="alert" role="alert">
									
										<!-- <label class="checkbox-wrap checkbox-primary">
											  <input type="checkbox" checked>
											  <span class="checkmark"></span>
											</label> -->
											
									
								  <td class="d-flex align-items-center">
									  <div class="img" style="background-image: url(assets-table/images/person_1.jpg);"></div>
									  <div class="pl-3 email">
										  <span><%= item.client_camp%></span>
										  <span>Starts: <%= item.startdate%> <br>
											Farouk Abubabkar Audu
										</span>
									  </div>
								  </td>
								
								  <td><%= item.state %></td>
								  <td><%= item.address.slice(0,4)%>...</td>
								  <td><%= item.structure %></td>
								  <td>“₦” <%= item.unit.toLocaleString("en-US") %></td>
								  <td><%= item.end_date %></td>
								  <% if(item.ads_status === "Running"){ %>
									<td class="status"><span class="active">Running <a href="#" class="fa fa-eye"></a></span></td>
								  <% }else{ %>
									<td class="status border-bottom-0"><span class="waiting" >Expired <a href="#" class="fa fa-eye"></a></span></td>
								  <% } %>
								  
								  <!-- <td>
									  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
									<span aria-hidden="true"><i class="fa fa-close"></i></span>
									<span aria-hidden="true"><i class="fa fa-eye"></i></span>
								  </button>
								  
								</td> -->
								</tr>
							

							<% }) %>

						   
						  </tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</section>

	<script src="assets-table/js/jquery.min.js"></script>
  <script src="assets-table/js/popper.js"></script>
  <script src="assets-table/js/bootstrap.min.js"></script>
  <script src="assets-table/js/main.js"></script>

	</body>
</html>

